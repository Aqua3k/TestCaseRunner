{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "contents": {
      "type": "object",
      "properties": {
        "testcase": {
          "type": "object",
          "patternProperties": {
            "^\\d+$": { "type": "string" }
          },
          "additionalProperties": false
        },
        "input_hash": {
          "type": "object",
          "patternProperties": {
            "^\\d+$": { "type": "string" }
          },
          "additionalProperties": false
        },
        "in": {
          "type": "object",
          "patternProperties": {
            "^\\d+$": { "type": "string" }
          },
          "additionalProperties": false
        },
        "stdout": {
          "type": "object",
          "patternProperties": {
            "^\\d+$": { "type": "string" }
          },
          "additionalProperties": false
        },
        "stderr": {
          "type": "object",
          "patternProperties": {
            "^\\d+$": { "type": "string" }
          },
          "additionalProperties": false
        },
        "status": {
          "type": "object",
          "patternProperties": {
            "^\\d+$": { "type": "integer" }
          },
          "additionalProperties": false
        }
      },
      "required": ["testcase", "input_hash", "in", "stdout", "stderr", "status"],
      "additionalProperties": true
    },
    "metadata": {
      "type": "object",
      "properties": {
        "library_name": { "type": "string" },
        "created_date": { "type": "string" },
        "testcase_num": { "type": "integer" },
        "attributes": {
          "type": "object",
          "properties": {
            "testcase": { "type": "integer" },
            "input_hash": { "type": "integer" },
            "in": { "type": "integer" },
            "stdout": { "type": "integer" },
            "stderr": { "type": "integer" },
            "status": { "type": "integer" }
          },
          "required": ["testcase", "input_hash", "in", "stdout", "stderr", "status"],
          "additionalProperties": true
        }
      },
      "required": ["library_name", "created_date", "testcase_num", "attributes"],
      "additionalProperties": false
    }
  },
  "required": ["contents", "metadata"],
  "additionalProperties": false
}
